<Window x:Class="Реестр_маневренного_фонда.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Реестр_маневренного_фонда"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <StackPanel Orientation = "Vertical">
            <Button Name = "bt_AgreementsViewPage" Content="Договоры" Click="bt_AgreementsViewPage_Click"/>
            <Button Name = "bt_DecreesViewPage"/>
            <Button Name = "bt_HousingFundViewPage"/>
            <Button Name = "bt_LocalitiesViewPage"/>
            <Button Name = "bt_ResidenceRegistrationsViewPage"/>
            <Button Name = "bt_StreetsViewPage"/>
            <Button Name = "bt_TempResidentsViewPage"/>
            <TextBox x:Name="tb" TextAlignment="Center" Text="00"/>
        </StackPanel>

        <Frame Grid.Row="1" x:Name = "fr_Frame" NavigationUIVisibility = "Hidden"/>
    </Grid>
</Window>
